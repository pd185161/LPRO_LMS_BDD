1 - Gram
2 - KG
3 - OZ
4 - Liter
5 - Gallon
6 - Lb</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:byte">
																		<xsd:enumeration value="1"/>
																		<xsd:enumeration value="2"/>
																		<xsd:enumeration value="3"/>
																		<xsd:enumeration value="4"/>
																		<xsd:enumeration value="5"/>
																		<xsd:enumeration value="6"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="PromotionFlowStatus" use="optional">
