<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2019 rel. 3 sp1 (http://www.altova.com) by NCR CORP (NCR CORP) -->
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<xsd:include schemaLocation="Includes/RestrictedDateAndDateTime.xsd"/>
	<xsd:element name="HQL_Promotion_Interface">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Retailer" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>Represents the Retailer Id which holds the promotions</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:all>
							<xsd:element name="PromotionGroups" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="PromotionGroup" maxOccurs="unbounded">
											<xsd:complexType>
												<xsd:attribute name="ExternalGroupId" use="required">
													<xsd:annotation>
														<xsd:documentation>PromotionGroup.ExternalGroupId</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:maxLength value="24"/>
															<xsd:minLength value="1"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:attribute>
												<xsd:attribute name="Name" use="required">
													<xsd:annotation>
														<xsd:documentation>PromotionGroup.PromotionGroupName</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:maxLength value="40"/>
															<xsd:minLength value="1"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:attribute>
												<xsd:attribute name="Description" use="optional">
													<xsd:annotation>
														<xsd:documentation>PromotionGroup.Remarks</xsd:documentation>
													</xsd:annotation>
												</xsd:attribute>
												<xsd:attribute name="ParentExternalGroupId" use="required">
													<xsd:annotation>
														<xsd:documentation>PromotionGroup.ParentGroupId FROM PromotionGroup WHERE PromotionGroup.ExternalGroupId = ImportPromotionGroups.ParentExternalGroupId</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:minLength value="1"/>
															<xsd:maxLength value="24"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:attribute>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="Promotions" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Promotions Data</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="Promotion" maxOccurs="unbounded">
											<xsd:complexType>
												<xsd:all>
													<xsd:element name="PromotionActiveDays" minOccurs="0">
														<xsd:annotation>
															<xsd:documentation>Populated into PromotionHeader table</xsd:documentation>
														</xsd:annotation>
														<xsd:complexType>
															<xsd:attribute name="StartTime" type="xsd:time" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.StartTime (HH:MM:SS)</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="EndTime" type="xsd:time" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.EndTime (HH:MM:SS)</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="ActiveOnSunday" type="xsd:boolean" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.ActiveOnSunday</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="ActiveOnMonday" type="xsd:boolean" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.ActiveOnMonday</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="ActiveOnTuesday" type="xsd:boolean" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.ActiveOnTuesday</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="ActiveOnWednesday" type="xsd:boolean" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.ActiveOnWednesday</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="ActiveOnThursday" type="xsd:boolean" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.ActiveOnThursday</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="ActiveOnFriday" type="xsd:boolean" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.ActiveOnFriday</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="ActiveOnSaturday" type="xsd:boolean" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.ActiveOnSaturday</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="EnhancedPromotionActiveDays" minOccurs="0">
														<xsd:annotation>
															<xsd:documentation>Populated into PromotionHeader table. CR LOYPRO-2889 Import Promotion Daily StartTime and EndTime</xsd:documentation>
														</xsd:annotation>
														<xsd:complexType>
															<xsd:attribute name="ActiveOnSunday" type="xsd:boolean" use="required"/>
															<xsd:attribute name="SundayStartTime" type="xsd:time" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.SundayStartTime (HH:MM:SS)</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="SundayEndTime" type="xsd:time" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.SundayEndTime (HH:MM:SS)</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="ActiveOnMonday" type="xsd:boolean" use="required"/>
															<xsd:attribute name="MondayStartTime" type="xsd:time" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.MondayStartTime (HH:MM:SS)</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="MondayEndTime" type="xsd:time" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.MondayEndTime (HH:MM:SS)</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="ActiveOnTuesday" type="xsd:boolean" use="required"/>
															<xsd:attribute name="TuesdayStartTime" type="xsd:time" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.TuesdayStartTime (HH:MM:SS)</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="TuesdayEndTime" type="xsd:time" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.TuesdayEndTime (HH:MM:SS)</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="ActiveOnWednesday" type="xsd:boolean" use="required"/>
															<xsd:attribute name="WednesdayStartTime" type="xsd:time" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.WednesdayStartTime (HH:MM:SS)</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="WednesdayEndTime" type="xsd:time" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.WednesdayEndTime (HH:MM:SS)</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="ActiveOnThursday" type="xsd:boolean" use="required"/>
															<xsd:attribute name="ThursdayStartTime" type="xsd:time" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.ThursdayStartTime (HH:MM:SS)</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="ThursdayEndTime" type="xsd:time" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.ThursdayEndTime (HH:MM:SS)</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="ActiveOnFriday" type="xsd:boolean" use="required"/>
															<xsd:attribute name="FridayStartTime" type="xsd:time" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.FridayStartTime (HH:MM:SS)</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="FridayEndTime" type="xsd:time" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.FridayEndTime (HH:MM:SS)</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="ActiveOnSaturday" type="xsd:boolean" use="required"/>
															<xsd:attribute name="SaturdayStartTime" type="xsd:time" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.SaturdayStartTime (HH:MM:SS)</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="SaturdayEndTime" type="xsd:time" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.SaturdayEndTime (HH:MM:SS)</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="PromotionDetails" minOccurs="0">
														<xsd:annotation>
															<xsd:documentation>Populated into PromotionHeader table</xsd:documentation>
														</xsd:annotation>
														<xsd:complexType>
															<xsd:attribute name="HeaderDescription" use="required">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.PromotionHeaderDescription</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:string">
																		<xsd:maxLength value="60"/>
																		<xsd:minLength value="1"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="Status" type="xsd:boolean" use="required">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.Status
0 - non active promotion
1 - active promotion</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="TypeId" type="xsd:int" use="required">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.PromotionTypeId:
1 - Single Item
2 - Single Condition
3 - Multi Condition
4 - Total Ticket
6 - Best Deal
11 -Manual Item Discount
12	- Manual Ticket Discount
13	- Targeted Manual Item Discount (RSO)
14	-Targeted Manual Item Discount
15 - Multi Reward Price Comparison
16 - Multi Reward
17 - Multi Condition - Multi Reward
18 - Manual UOM Item Discount</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="StartDate" type="restrictedDateTime" use="required">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.PromotionHeaderStartDate
YYYY-MM-DDTHH:MM:SS</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="EndDate" type="restrictedDateTime" use="required">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.PromotionHeaderEndDate
YYYY-MM-DDTHH:MM:SS</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="TargetPopulationType" use="required">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.TargetPopulationType
1 - All Customers
2 - All Members
3 - Segments
4 - Non Members</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:int">
																		<xsd:enumeration value="1"/>
																		<xsd:enumeration value="2"/>
																		<xsd:enumeration value="3"/>
																		<xsd:enumeration value="4"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="RedemptionLimitMaximum" type="xsd:int" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.RedemptionLimitDuringPromotionPeriod</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="RedemptionLimitPerDay" type="xsd:int" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.RedemptionLimitDuringSingleDay</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="RedemptionLimitPerTransaction" type="xsd:int" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.RedemptionLimitDuringSingleTicket</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="MinimumPurchaseAmount" type="xsd:float" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.MinimumPurchaseAmount</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="Remarks" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.Remarks</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:string">
																		<xsd:maxLength value="500"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="MarkdownCategoryId" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.MarkdownCategoryKey</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:string">
																		<xsd:maxLength value="24"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="MarkdownCategoryLevelId" type="xsd:int" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.MarkdownCategoryKey</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="LimitedToHomeStoreOnly" type="xsd:boolean" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.LimitToHomeStoreOnly</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="SponsorSupplierId" use="optional">
																<xsd:annotation>
																	<xsd:documentation>Converted To PromotionHeader.SponsorSupplierInternalKey FROM HOST.SupplierCode WHERE SupplierCode.SupplierId = SponsorSupplierId AND SupplierCode.MatrixMemberId = imported promotion MatrixMemberId</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:string">
																		<xsd:maxLength value="24"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="SharedFundingValue" type="xsd:float" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.SharedFundingValue</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="PromotionReceiptDescription" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.PromotionReceiptDescription</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:string">
																		<xsd:maxLength value="40"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="AdditionalTypeId" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.AdditionalType
0 - General
Currently only additional type 0 is supported</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:int">
																		<xsd:enumeration value="0"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="ContinuityScope" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.ThresholdContinuityStatus
1 - ContinuitySingleTicket
2 - ContinuityAccomulate

Value 2 allowed only in case:
TargetPopulationType in (2,3)
and
PopulationOfflineMode = 1
and
TresholdTypeId in (1,2,3,6,4)
and
RewardTemplateId not in (1,10,52,54,51,57)</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:int">
																		<xsd:enumeration value="1"/>
																		<xsd:enumeration value="2"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="TriggerTiming" type="xsd:boolean" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.TriggerImmediatelyUponMeetingCondition
1 - immediate triggering when items is scanned
0 - delayed triggering at end of ticket.Default value get from system parameter  &quot;Default_PromotionCalculation_Timing&quot;</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="DiscountAllocationFlag" type="xsd:boolean" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.DiscountAllocationFlag
1 or missing - Checked
0 - Unchecked
																	</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="DiscountAllocationScope" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.DiscountAllocationScope
1 - Department (refer to MarkdownCategoryId, MarkdownCategoryLevelId)
2 - Single Item (From bucket)
3 - Include all items
4 - Include only the trigger items
5 - Include only the RewardedItems</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:int">
																		<xsd:enumeration value="1"/>
																		<xsd:enumeration value="2"/>
																		<xsd:enumeration value="3"/>
																		<xsd:enumeration value="4"/>
																		<xsd:enumeration value="5"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="CouponRewardsScope" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.CouponRewardsScope
1 - Single Coupon
2 - Separate Coupon</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:int">
																		<xsd:enumeration value="1"/>
																		<xsd:enumeration value="2"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="PromotionFundingScope" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.PromotionFundingScope
1 - Retailer funding only
2 - Supplier funding only (refer to SponserSupplierId)
3 - Share funding (refer to  SharedFundingScope,SharedFundingValue)
4 - Store funding only
5 - None</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:int">
																		<xsd:enumeration value="1"/>
																		<xsd:enumeration value="2"/>
																		<xsd:enumeration value="3"/>
																		<xsd:enumeration value="4"/>
																		<xsd:enumeration value="5"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="SharedFundingScope" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.SharedFundingScope
1 - Amount
2 - Percent
(In case needed but not sent in the file set to 1, according to GUI business rules)</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:int">
																		<xsd:enumeration value="1"/>
																		<xsd:enumeration value="2"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="TicketPrintingScope" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.TicketPrintingScope
0 - Apply Discount On Promotion Description
1 - Apply Discount On Coupon
2 - Print Rewarded Item Desc As Promo Desc
3 - Free text
4 - Do Not Apply Discount Printing In Ticket
5 - Act by touchpoint logic
6 - Apply Discount On Promotion Description with Qualify Spend – supported only for TypeId = 1,2,3 &amp;18 </xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:int">
																		<xsd:enumeration value="0"/>
																		<xsd:enumeration value="1"/>
																		<xsd:enumeration value="2"/>
																		<xsd:enumeration value="3"/>
																		<xsd:enumeration value="4"/>
																		<xsd:enumeration value="5"/>
																		<xsd:enumeration value="6"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="MeanOfPaymentScope" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.MeanOfPaymentScope
1 - MeanOfPayment
2 - Discount
3 - MeanOfPaymentDiscount
Currently only value 2 is supported.</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:int">
																		<xsd:enumeration value="1"/>
																		<xsd:enumeration value="2"/>
																		<xsd:enumeration value="3"/>
																		<xsd:enumeration value="4"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="ConflictScope" use="optional" default="1">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.PromotionPriority
1 - Never participate in conflict
2 - Always wins
3 - Normal
4 - Always loses
5 - Participate partial reward (Relevant only for Treshold.TriggerType = 8 - case quantity)
6 - Advanced optimization</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:int">
																		<xsd:enumeration value="1"/>
																		<xsd:enumeration value="2"/>
																		<xsd:enumeration value="3"/>
																		<xsd:enumeration value="4"/>
																		<xsd:enumeration value="5"/>
																		<xsd:enumeration value="6"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="PopulationSegmentsOperator" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.PopulationSegmentsOperator
1 - OR
2 - AND</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:int">
																		<xsd:enumeration value="1"/>
																		<xsd:enumeration value="2"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="PopulationLocalSegmentsOperator" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.PopulationLocalSegmentsOperator
1 - OR
2 - AND</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:int">
																		<xsd:enumeration value="1"/>
																		<xsd:enumeration value="2"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="PopulationOfflineMode" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.PopulationOfflineMode
1 - Online only
2 - Also in offline
3 - Only in offline
4 - In offline disregard member segment condition</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:int">
																		<xsd:enumeration value="1"/>
																		<xsd:enumeration value="2"/>
