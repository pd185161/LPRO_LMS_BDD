																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="RegisteredPromotionRedemption" use="optional">
																<xsd:annotation>
																	<xsd:documentation>
																		PromotionRegistration.RegisteredPromotionRedemption FROM ImportPromotionDetails WHERE ImportPromotionDetails.PromotionHeaderId_IK = PromotionRegistration.PromotionHeaderId
																		FSD 441814
																		1 = Registered Promotion is set to Single Use
																		0 = Unlimited
																		2 = Redemptions are set per Household (CR 522735)
																	</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:int">
																		<xsd:enumeration value="0"/>
																		<xsd:enumeration value="1"/>
																		<xsd:enumeration value="2"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="DeductRegistrationCost" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionRegistration.DeductRegistrationCost FROM ImportPromotionDetails WHERE ImportPromotionDetails.PromotionHeaderId_IK = PromotionRegistration.PromotionHeaderId</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:short">
																		<xsd:enumeration value="0"/>
																		<xsd:enumeration value="1"/>
																	</xsd:restriction>
