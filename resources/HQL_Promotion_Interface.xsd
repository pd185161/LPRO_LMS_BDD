																	<xsd:documentation>PromotionRegistration.MemberAccountId FROM ImportPromotionDetails WHERE ImportPromotionDetails.PromotionHeaderId_IK = PromotionRegistration.PromotionHeaderId</xsd:documentation>
																</xsd:annotation>
