													<xsd:element name="PromotionsBuckets" minOccurs="0">
														<xsd:annotation>
															<xsd:documentation>Populated into PromotiomBucket table</xsd:documentation>
														</xsd:annotation>
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="PromotionsBucket" maxOccurs="unbounded">
																	<xsd:complexType>
																		<xsd:sequence>
																			<xsd:element name="BucketEntity" minOccurs="0" maxOccurs="unbounded">
																				<xsd:annotation>
																					<xsd:documentation>Populated into PromotionBucketEntityTable</xsd:documentation>
