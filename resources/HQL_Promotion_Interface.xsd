0 - Not Excluded
1 - Excluded</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:int">
																		<xsd:enumeration value="0"/>
																		<xsd:enumeration value="1"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
