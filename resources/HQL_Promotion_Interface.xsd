																				<xsd:complexType>
																					<xsd:attribute name="EntityId" use="required">
																						<xsd:annotation>
																							<xsd:documentation>PromotionBucketEntity.EntityInternalkey FROM HOST table, depending on EntityTypeId</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:string">
																								<xsd:maxLength value="24"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="EntityType" use="required">
																						<xsd:annotation>
																							<xsd:documentation>PromotionBucketEntity.EntityTypeId
0 - Item
2 - Category
3 - Supplier
6 - Tender
14 - Dynamic Attribute
46 – Tender BIN
</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:int">
																								<xsd:enumeration value="0"/>
																								<xsd:enumeration value="2"/>
																								<xsd:enumeration value="3"/>
																								<xsd:enumeration value="6"/>
																								<xsd:enumeration value="14"/>
																								<xsd:enumeration value="46"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="EntityLevel" type="xsd:int" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>Not saved in Promotion tables, but used (and mandatory) to find the category when EntityType = 2</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="EntityValue" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionBucketEntity.EntityValue
Relevant If EntityType = 14 (DynamicAttribute)</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:string">
																								<xsd:maxLength value="100"/>
																								<xsd:pattern value="[ -!#-%(-;=?-~]{1,100}"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="EntityOperator" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionBucketEntity.EntityOperator
Relevant and mandatory when EntityType = 14 (Dynamic Attribute)
Only supported value is &quot;=&quot;
Relevant and mandatory when EntityType = 46 (Tender BIN)
supported values are &quot;=&quot;  OR &quot;Between&quot;  OR &quot;StartsWith&quot; 


</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:string">
																								<xsd:maxLength value="10"/>
																								<xsd:enumeration value="="/>
																								<xsd:enumeration value="Between"/>
																								<xsd:enumeration value="StartsWith"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="EntityAdditionalValue" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>
PromotionBucketEntity.EntityAdditionalValue
Relevant If EntityType = 14 (DynamicAttribute)  OR EntityType=46(Tender BIN)  AND EntityOperator = &quot;Between&quot;
</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:string">
																								<xsd:maxLength value="100"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="AndOrOperator" type="xsd:boolean" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionBucketEntity.EntityAndOrOperator
0 - AND
1 - OR</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="Exclude" type="xsd:boolean" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionBucketEntity.Exclude
1 - Exclude
0 - Not excluded</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="MultiRewardValue" type="xsd:float" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionBucketEntity.MultiRewardValue
Upon saving a promotion of Multi Reward Price Comparison promotion type, the promotion will be saved with multiple reward values, one per each item in the bucket</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="EntityAdditionalName" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionCompetitorItems.CompetitorItemDescription</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:string">
																								<xsd:maxLength value="160"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="ItemRedemptionLimit" type="xsd:int" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionBucketEntity.ItemRedemptionLimit
Redemption limit at bucket item level (Upon saving a promotion of type Multi Condition-Multi Reward, the Quantity bucket will be saved with redemption limit per item)</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="EntityViewId" type="xsd:short">
																						<xsd:annotation>
																							<xsd:documentation>Not saved in Promotion tables, but usedto identify the correct Hierarchy when EntityTypeId = 2</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																				</xsd:complexType>
																			</xsd:element>
																		</xsd:sequence>
																		<xsd:attribute name="Id" type="xsd:int" use="required">
																			<xsd:annotation>
																				<xsd:documentation>PromotionBucket.BucketId</xsd:documentation>
																			</xsd:annotation>
																		</xsd:attribute>
																		<xsd:attribute name="Name" use="optional">
																			<xsd:annotation>
																				<xsd:documentation>PromotionBucket.BucketName</xsd:documentation>
																			</xsd:annotation>
																			<xsd:simpleType>
																				<xsd:restriction base="xsd:string">
																					<xsd:maxLength value="40"/>
																				</xsd:restriction>
																			</xsd:simpleType>
																		</xsd:attribute>
																		<xsd:attribute name="QtySize" type="xsd:int" use="optional">
																			<xsd:annotation>
																				<xsd:documentation>PromotionBucket.QtySize</xsd:documentation>
																			</xsd:annotation>
																		</xsd:attribute>
																		<xsd:attribute name="QtySizeMinValue" type="xsd:float" use="optional">
																			<xsd:annotation>
																				<xsd:documentation>PromotionBucket.QtySizeMinValue</xsd:documentation>
																			</xsd:annotation>
																		</xsd:attribute>
																		<xsd:attribute name="QtySizeMaxValue" type="xsd:float" use="optional">
																			<xsd:annotation>
																				<xsd:documentation>PromotionBucket.QtySizeMaxValue</xsd:documentation>
																			</xsd:annotation>
																		</xsd:attribute>
																		<xsd:attribute name="MinimumRequiredValue" type="xsd:float" use="optional">
																			<xsd:annotation>
																				<xsd:documentation>PromotionBucket.MinimumRequiredValue</xsd:documentation>
																			</xsd:annotation>
																		</xsd:attribute>
																		<xsd:attribute name="ThresholdAppliesTo" use="optional">
																			<xsd:annotation>
																				<xsd:documentation>PromotionBucket.ThresholdAppliesTo
In case ThresholdTypeId != 5 and ThresholdTypeId != 9 and ThresholdTypeId != 10
0 - Any item
1 - Same items
2 - Different items
3 - Same Internal Number
4 - Different Internal Number

In case ThresholdTypeId = 5
0 - Current balance
101 - Opening balance
102 - Earning this visit
103 - Redemption This Visit - unused for now, will pass schema validation but will fail stp_ImportPromotions_Validation

In case ThresholdTypeId = 9 Or 10
201 - Any Tender</xsd:documentation>
																			</xsd:annotation>
																			<xsd:simpleType>
																				<xsd:restriction base="xsd:int">
																					<xsd:enumeration value="0"/>
																					<xsd:enumeration value="1"/>
																					<xsd:enumeration value="2"/>
																					<xsd:enumeration value="3"/>
																					<xsd:enumeration value="4"/>
																					<xsd:enumeration value="101"/>
																					<xsd:enumeration value="102"/>
																					<xsd:enumeration value="103"/>
																					<xsd:enumeration value="201"/>
																				</xsd:restriction>
																			</xsd:simpleType>
																		</xsd:attribute>
																		<xsd:attribute name="ParticipateInReward" type="xsd:boolean" use="optional">
																			<xsd:annotation>
																				<xsd:documentation>PromotionBucket.ParticipateInReward
0 - not participating
1 - participating</xsd:documentation>
																			</xsd:annotation>
																		</xsd:attribute>
																		<xsd:attribute name="IncentiveMsgAppliesTo" use="optional">
																			<xsd:annotation>
																				<xsd:documentation>PromotionBucket.IncentiveMsgAppliesTo
NULL - There is no Incentive Message
1 - Contains Incentive Message</xsd:documentation>
																			</xsd:annotation>
																			<xsd:simpleType>
																				<xsd:restriction base="xsd:int">
																					<xsd:enumeration value="1"/>
																				</xsd:restriction>
																			</xsd:simpleType>
																		</xsd:attribute>
																		<xsd:attribute name="RewardCalculationStartsFromMinValue" use="optional">
																			<xsd:annotation>
																				<xsd:documentation>PromotionBucket.RewardCalculationStartsFromMinValue
0 - Reward Calculation starts from 0
1 - Reward Calculation starts from minimum Value</xsd:documentation>
																			</xsd:annotation>
																			<xsd:simpleType>
																				<xsd:restriction base="xsd:int">
																					<xsd:enumeration value="0"/>
																					<xsd:enumeration value="1"/>
																				</xsd:restriction>
																			</xsd:simpleType>
																		</xsd:attribute>
																		<xsd:attribute name="AndOrOperator" type="xsd:boolean" use="optional">
																			<xsd:annotation>
																				<xsd:documentation>PromotionBucket.AndOrOperator
0 - AND
1 - OR</xsd:documentation>
																			</xsd:annotation>
																		</xsd:attribute>
																		<xsd:attribute name="AllocateDiscountBreakdown" type="xsd:boolean" use="optional">
																			<xsd:annotation>
																				<xsd:documentation>PromotionBucket.AllocateDiscountBreakdown
0 - No allocation
1 - Allocation</xsd:documentation>
																			</xsd:annotation>
																		</xsd:attribute>
																		<xsd:attribute name="RoundingToMultipleOf" type="xsd:float" use="optional">
																			<xsd:annotation>
																				<xsd:documentation>PromotionBucket.RoundingToMultipleOf
Sets the rounding rule for promotion triggering purposes</xsd:documentation>
																			</xsd:annotation>
																		</xsd:attribute>
																		<xsd:attribute name="LimitRewardedByConditionSpend" type="xsd:boolean" use="optional">
																			<xsd:annotation>
																				<xsd:documentation>PromotionBucket.LimitRewardedByConditionSpend
When the flag’s value = TRUE, then the LPE will apply the reward only on the quantity/volume, derived from the total spend in the fixed condition</xsd:documentation>
																			</xsd:annotation>
																		</xsd:attribute>
																		<xsd:attribute name="BucketTriggerItemsWillBeExcludedFromOtherPromotions" type="xsd:boolean" use="optional">
																			<xsd:annotation>
																				<xsd:documentation>PromotionBucket.BucketTriggerItemsWillBeExcludedFromOtherPromotions</xsd:documentation>
																			</xsd:annotation>
																		</xsd:attribute>
																		<xsd:attribute name="BucketExcludeTriggerItemsOfMarkedPromotions" type="xsd:boolean" use="optional">
																			<xsd:annotation>
																				<xsd:documentation>PromotionBucket.BucketExcludeTriggerItemsOfMarkedPromotions</xsd:documentation>
																			</xsd:annotation>
																		</xsd:attribute>
																		<xsd:attribute name="DeductTenderAmountFromQualifyingTicketSpend" type="xsd:boolean" use="optional">
																			<xsd:annotation>
																				<xsd:documentation>PromotionBucket.DeductTenderAmountFromQualifyingTicketSpend
Deduct tender amount from Ticket qualifying Spend, for &apos;Ticket Spend&apos; condition type, only. CR 519878.</xsd:documentation>
																			</xsd:annotation>
																		</xsd:attribute>
																		<xsd:attribute name="ApplyOnGrossTenderAmount" type="xsd:boolean" use="optional">
																			<xsd:annotation>
																				<xsd:documentation>PromotionBucket.ApplyOnGrossTenderAmount	LOYPRO-3589 - Tender Based Promotion Based on Transaction Gross Amount</xsd:documentation>
																			</xsd:annotation>
																		</xsd:attribute>
																	</xsd:complexType>
																</xsd:element>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="PromotionLoyaltySegments" minOccurs="0">
														<xsd:annotation>
															<xsd:documentation>Populated into PromotionSegments table</xsd:documentation>
														</xsd:annotation>
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="PromotionLoyaltySegment" maxOccurs="unbounded">
																	<xsd:complexType>
																		<xsd:attribute name="SegmentId" use="required">
																			<xsd:simpleType>
																				<xsd:annotation>
																					<xsd:documentation>PromotionSegments.SegmentInternalkey FROM LOYALTY.CRM_Segment WHERE CRM_Segment.ClubInternalKey = ImportPromotions_LoyaltySegments.ClubInternalKey AND CRM_Segment.SegmentId = ImportPromotions_LoyaltySegments.SegmentId
Carries SegmentId value in case attribute ElementType = 0 (segment) and carries Operator value in case attribute ElementType = 1 (opeartor).
The possible operator values are:
1 - AND
2 - OR
3 - NOT
4 - (
5 - )</xsd:documentation>
																				</xsd:annotation>
																				<xsd:restriction base="xsd:int">
																					<xsd:minInclusive value="1"/>
																					<xsd:maxInclusive value="999999999"/>
																				</xsd:restriction>
																			</xsd:simpleType>
																		</xsd:attribute>
																		<xsd:attribute name="Exclude" type="xsd:boolean" use="required">
																			<xsd:annotation>
																				<xsd:documentation>PromotionSegments.Exclude
1 - Exclude
0 - Include</xsd:documentation>
																			</xsd:annotation>
																		</xsd:attribute>
																		<xsd:attribute name="LineId" type="xsd:int" use="optional">
																			<xsd:annotation>
																				<xsd:documentation>PromotionSegments.LineId
Relevant, mandatory and unique for advanced segmentation mode</xsd:documentation>
																			</xsd:annotation>
																		</xsd:attribute>
																		<xsd:attribute name="ElementType" use="optional">
																			<xsd:annotation>
																				<xsd:documentation>PromotionSegments.ElementType
0 - segment
1 - operator
Relevant and mandatory for advanced segmentation mode</xsd:documentation>
																			</xsd:annotation>
																			<xsd:simpleType>
																				<xsd:restriction base="xsd:int">
																					<xsd:enumeration value="0"/>
																					<xsd:enumeration value="1"/>
																				</xsd:restriction>
																			</xsd:simpleType>
																		</xsd:attribute>
																	</xsd:complexType>
																</xsd:element>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="LinkedPromotions" minOccurs="0">
														<xsd:annotation>
															<xsd:documentation>Populated into PromotionLinkedPromotion table</xsd:documentation>
														</xsd:annotation>
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="LinkedPromotion" maxOccurs="unbounded">
																	<xsd:complexType>
																		<xsd:attribute name="LinkedPromotionId" use="required">
																			<xsd:annotation>
																				<xsd:documentation>PromotionLinkedPromotion.LinkedPromotionHeaderId WHERE ((ImportPromotions_LinkedPromotion.MatrixMemberId = PromotionHeader.MatrixMemberId AND ImportPromotions_LinkedPromotion.LinkedPromotionId_IK = PromotionHeader.PromotionHeaderId) OR ImportPromotions_LinkedPromotion.MatrixMemberId = ImportPromotionDetails.MatrixMemberId AND ImportPromotions_LinkedPromotion.LinkedPromotionId_IK = ImportPromotionDetails.PromotionHeaderId_IK))</xsd:documentation>
																			</xsd:annotation>
																			<xsd:simpleType>
																				<xsd:restriction base="xsd:string">
																					<xsd:maxLength value="24"/>
																				</xsd:restriction>
																			</xsd:simpleType>
																		</xsd:attribute>
																	</xsd:complexType>
																</xsd:element>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="PromotionAttributes" minOccurs="0">
														<xsd:annotation>
															<xsd:documentation>Populated into PromotionAttributeValue table</xsd:documentation>
														</xsd:annotation>
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="Attribute" maxOccurs="unbounded">
																	<xsd:complexType>
																		<xsd:attribute name="Id" type="xsd:int" use="required">
																			<xsd:annotation>
																				<xsd:documentation>PromotionAttributeValue.AttributeId</xsd:documentation>
																			</xsd:annotation>
																		</xsd:attribute>
																		<xsd:attribute name="Value" use="required">
																			<xsd:annotation>
																				<xsd:documentation>PromotionAttributeValue table:
Field LongStringValue WHEN ImportPromotions_PromotionAttributesAttributeDataType = 7
Field StringValue WHEN ImportPromotions_PromotionAttributes.AttributeDataType = 1
Field LongValue WHEN ImportPromotions_PromotionAttributes.AttributeDataType = 2
Field MoneyValue WHEN ImportPromotions_PromotionAttributes.AttributeDataType = 3
Field DoubleValue WHEN ImportPromotions_PromotionAttributes.AttributeDataType = 4
Field DateValue WHEN ImportPromotions_PromotionAttributes.AttributeDataType = 5
Field BooleanValue WHEN ImportPromotions_PromotionAttributes.AttributeDataType = 6</xsd:documentation>
																			</xsd:annotation>
																			<xsd:simpleType>
																				<xsd:restriction base="xsd:string">
																					<xsd:maxLength value="1024"/>
																					<xsd:minLength value="1"/>
																				</xsd:restriction>
																			</xsd:simpleType>
																		</xsd:attribute>
																	</xsd:complexType>
																</xsd:element>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="Translations" minOccurs="0">
														<xsd:annotation>
															<xsd:documentation>Populated into TranslatedValues table</xsd:documentation>
														</xsd:annotation>
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="Attribute" maxOccurs="unbounded">
																	<xsd:complexType>
																		<xsd:sequence>
																			<xsd:element name="Language" minOccurs="0" maxOccurs="unbounded">
																				<xsd:annotation>
																					<xsd:documentation>Element inside TranslationXML column in TranslatedValues table</xsd:documentation>
																				</xsd:annotation>
																				<xsd:complexType>
																					<xsd:attribute name="LanguageId" type="xsd:int" use="required">
																						<xsd:annotation>
																							<xsd:documentation>Unique id of Language inside TranslationXML column in TranslatedValues table</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="Value" type="xsd:string" use="required">
																						<xsd:annotation>
																							<xsd:documentation>Translated value according to LanguageId.</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																				</xsd:complexType>
																			</xsd:element>
																		</xsd:sequence>
																		<xsd:attribute name="AttributeId" type="xsd:string" use="required">
																			<xsd:annotation>
																				<xsd:documentation>Attribute Id in Entity of Translation</xsd:documentation>
																			</xsd:annotation>
																		</xsd:attribute>
																	</xsd:complexType>
																</xsd:element>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="CheckoutGroups" minOccurs="0">
														<xsd:annotation>
															<xsd:documentation>Populated into PromotionCheckoutGroup table</xsd:documentation>
														</xsd:annotation>
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="CheckoutGroup" maxOccurs="unbounded">
																	<xsd:complexType>
																		<xsd:attribute name="CheckoutGroupId" use="required">
																			<xsd:annotation>
																				<xsd:documentation>PromotionCheckoutGroup.CheckoutGroupInternalKey FROM HOST.CHeckhoutGroup WHERE ImportPromotions_CheckoutGroups.MatrixmemerId = CheckoutGroups.MatrixMemberId AND ImportPromotions_CheckoutGroups.CheckoutGroupId = CheckoutGroups.CheckoutGroupId</xsd:documentation>
																			</xsd:annotation>
																			<xsd:simpleType>
																				<xsd:restriction base="xsd:short">
																					<xsd:minInclusive value="1"/>
																					<xsd:maxInclusive value="99"/>
																				</xsd:restriction>
																			</xsd:simpleType>
																		</xsd:attribute>
																		<xsd:attribute name="Exclude" use="required">
																			<xsd:annotation>
																				<xsd:documentation>PromotionCheckoutGroup.Exclude
1 - Excluded
0 - Included</xsd:documentation>
																			</xsd:annotation>
																			<xsd:simpleType>
																				<xsd:restriction base="xsd:byte">
																					<xsd:enumeration value="0"/>
																					<xsd:enumeration value="1"/>
																				</xsd:restriction>
																			</xsd:simpleType>
																		</xsd:attribute>
																	</xsd:complexType>
																</xsd:element>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="MarketingAttributes" minOccurs="0">
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="Channels">
																	<xsd:annotation>
																		<xsd:documentation>Populated into PromotionMarketingChannels table</xsd:documentation>
																	</xsd:annotation>
																	<xsd:complexType>
																		<xsd:sequence>
																			<xsd:element name="Channel" maxOccurs="unbounded">
																				<xsd:complexType>
																					<xsd:attribute name="ChannelId" type="xsd:integer" use="required">
																						<xsd:annotation>
																							<xsd:documentation>PromotionMarketingChannels.MarketingChannelId
1 -Targeted Marketing
2 - Portal
3 - Mobile</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																				</xsd:complexType>
																			</xsd:element>
																		</xsd:sequence>
																	</xsd:complexType>
																</xsd:element>
																<xsd:element name="MarketingProperties">
																	<xsd:annotation>
																		<xsd:documentation>Populated into PromotionMarketingAttributes table</xsd:documentation>
																	</xsd:annotation>
																	<xsd:complexType>
																		<xsd:attribute name="Title" use="required">
																			<xsd:annotation>
																				<xsd:documentation>PromotionMarketingAttributes.Title</xsd:documentation>
																			</xsd:annotation>
																			<xsd:simpleType>
																				<xsd:restriction base="xsd:string">
																					<xsd:minLength value="1"/>
																				</xsd:restriction>
																			</xsd:simpleType>
																		</xsd:attribute>
																		<xsd:attribute name="Description" type="xsd:string" use="optional">
																			<xsd:annotation>
																				<xsd:documentation>PromotionMarketingAttributes.Description</xsd:documentation>
																			</xsd:annotation>
																		</xsd:attribute>
																		<xsd:attribute name="ImageLink" type="xsd:string" use="optional">
																			<xsd:annotation>
																				<xsd:documentation>PromotionMarketingAttributes.ImageLink
Link between the promotion and an image with marketing message</xsd:documentation>
																			</xsd:annotation>
																		</xsd:attribute>
																		<xsd:attribute name="Link" type="xsd:string" use="optional">
																			<xsd:annotation>
																				<xsd:documentation>PromotionMarketingAttributes.HyperLink
Link between the promotion and a page in the website with marketing message</xsd:documentation>
																			</xsd:annotation>
																		</xsd:attribute>
																		<xsd:attribute name="Restrictions" type="xsd:string" use="optional">
																			<xsd:annotation>
																				<xsd:documentation>PromotionMarketingAttributes.Restrictions
Promotion&apos;s restrictions (redemption limit, expiration, etc.)</xsd:documentation>
																			</xsd:annotation>
																		</xsd:attribute>
																		<xsd:attribute name="MarketingPriority" type="xsd:integer" use="optional">
																			<xsd:annotation>
																				<xsd:documentation>PromotionMarketingAttributes.Priority
Configurable list that defines the promotion’s priority</xsd:documentation>
																			</xsd:annotation>
																		</xsd:attribute>
																		<xsd:attribute name="SavingValue" type="xsd:decimal" use="optional"/>
																	</xsd:complexType>
																</xsd:element>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="PromotionSchemes" minOccurs="0">
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="PromotionScheme" maxOccurs="unbounded">
																	<xsd:complexType>
																		<xsd:attribute name="SchemeId" type="xsd:short" use="required">
																			<xsd:annotation>
																				<xsd:documentation>PromotionSchemes.SchemeInternalKey FROM Scheme WHERE Sheme.MatrixMemberId = ImportPromotions_PromotionSchemes.MatrixMemberId AND Sheme.SchemeId = ImportPromotions_PromotionSchemes.SchemeId AND Sheme.SchemeTypeId = ImportPromotions_PromotionSchemes.SchemeTypeId</xsd:documentation>
																			</xsd:annotation>
																		</xsd:attribute>
																		<xsd:attribute name="SchemeTypeId" type="xsd:short" use="required">
																			<xsd:annotation>
																				<xsd:documentation>PromotionSchemes.SchemeTypId</xsd:documentation>
																			</xsd:annotation>
																		</xsd:attribute>
																	</xsd:complexType>
																</xsd:element>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
												</xsd:all>
												<xsd:attribute name="PromotionHeaderId" use="required">
													<xsd:annotation>
														<xsd:documentation>PromotionHeader.ExternalReferenceId</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:maxLength value="50"/>
															<xsd:minLength value="1"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:attribute>
												<xsd:attribute name="Action" use="optional">
													<xsd:annotation>
														<xsd:documentation>This attribute indicate which action to be performed:
S - Suspend all the stores in the promotion distribution
US - Un-suspend all the stores in the promotion distribution. For regular insert/update of promotion this attribute shouldn&apos;t be sent.
DU - Distribution Update only</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:maxLength value="2"/>
															<xsd:enumeration value="S"/>
															<xsd:enumeration value="US"/>
															<xsd:enumeration value="DU"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:attribute>
											</xsd:complexType>
										</xsd:element>
										<xsd:element name="Legend" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Default values of unknown Promotion attributes when values are not sent in the import file</xsd:documentation>
											</xsd:annotation>
											<xsd:complexType>
												<xsd:all>
													<xsd:element name="PromotionActiveDays" minOccurs="0">
														<xsd:complexType>
															<xsd:attribute name="ActiveOnSunday" type="xsd:boolean" use="optional" default="1"/>
															<xsd:attribute name="ActiveOnMonday" type="xsd:boolean" use="optional" default="1"/>
															<xsd:attribute name="ActiveOnTuesday" type="xsd:boolean" use="optional" default="1"/>
															<xsd:attribute name="ActiveOnWednesday" type="xsd:boolean" use="optional" default="1"/>
															<xsd:attribute name="ActiveOnThursday" type="xsd:boolean" use="optional" default="1"/>
															<xsd:attribute name="ActiveOnFriday" type="xsd:boolean" use="optional" default="1"/>
															<xsd:attribute name="ActiveOnSaturday" type="xsd:boolean" use="optional" default="1"/>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="PromotionDetails" minOccurs="0">
														<xsd:complexType>
															<xsd:attribute name="AdditionalTypeId" use="optional" default="0">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.AdditionalType
0 - General</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:int">
																		<xsd:enumeration value="0"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="ContinuityScope" use="optional" default="1">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.ThresholdContinuityStatus
1 - ContinuitySingleTicket
2 - ContinuityAccomulate

Value 2 allowed only in case:
TargetPopulationType in (2,3) AND PopulationOfflineMode = 1 AND TresholdTypeId in (1,2,3,6) AND RewardTemplateId not in (1,10,52,54,60,61,51,57)</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:int">
																		<xsd:enumeration value="1"/>
																		<xsd:enumeration value="2"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="TriggerTiming" type="xsd:boolean" use="optional" default="0">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.TriggerImmediatelyUponMeetingCondition
1 – True, i.e. immediate triggering when items is scanned
0 – False, i.e. delayed triggering at end of ticket.Default value get from system parameter &quot;Default_PromotionCalculation_Timing&quot;</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="DiscountAllocationScope" use="optional" default="5">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.DiscountAllocationScope
1 - Department (refer to MarkdownCategoryId, MarkdownCategoryLevelId)
2 - Single Item (From bucket)
3 - Include all items
4 - Include only the trigger items
5 - Include only the RewardedItems</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:int">
																		<xsd:enumeration value="1"/>
																		<xsd:enumeration value="2"/>
																		<xsd:enumeration value="3"/>
																		<xsd:enumeration value="4"/>
																		<xsd:enumeration value="5"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="CouponRewardsScope" use="optional" default="2">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.CouponRewardsScope
1 - Single Coupon
2 - Separate Coupon</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:int">
																		<xsd:enumeration value="1"/>
																		<xsd:enumeration value="2"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="PromotionFundingScope" use="optional" default="1">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.PromotionFundingScope
1 - Retailer funding only
2 - Supplier funding only (refer to SponserSupplierId)
3 - Share funding (refer to  SharedFundingScope,SharedFundingValue)
4 - Store funding only</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:int">
																		<xsd:enumeration value="1"/>
																		<xsd:enumeration value="2"/>
																		<xsd:enumeration value="3"/>
																		<xsd:enumeration value="4"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="SharedFundingScope" use="optional" default="1">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.SharedFundingScope
1 - Amount
2 - Percent
In case needed but not sent in the file set to 1, according to GUI business rules</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:int">
																		<xsd:enumeration value="1"/>
																		<xsd:enumeration value="2"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="TicketPrintingScope" use="optional" default="0">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.TicketPrintingScope
0 - Apply Discount On Promotion Description
1 - Apply Discount On Coupon
2 - Print Rewarded Item Desc As Promo Desc
3 - Free text (currently not supported by import)
5 - Act by touchpoint logic</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:int">
																		<xsd:enumeration value="0"/>
																		<xsd:enumeration value="1"/>
																		<xsd:enumeration value="2"/>
																		<xsd:enumeration value="5"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="MeanOfPaymentScope" use="optional" default="2">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.MeanOfPaymentScope
1 - MeanOfPayment
2 - Discount
3 - MeanOfPaymentDiscount
4 - Discount, Included as Tender for Tender promotions
Currently only value 2 is supported.</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:int">
																		<xsd:enumeration value="1"/>
																		<xsd:enumeration value="2"/>
																		<xsd:enumeration value="3"/>
																		<xsd:enumeration value="4"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="ConflictScope" use="optional" default="1">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.PromotionPriority
1 - Exclude From Conflict
2 - Always Wins
3 - Default Rules
4 - Always Loses
5 - Participate partial reward (Relevant only for Treshold.TriggerType = 8 - case quantity)
6 - Advanced Optimization
If PromotionHeader.PromotionPriority is not equal 1 then its value is Overriding according to business rules defined in the GUI</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:int">
																		<xsd:enumeration value="1"/>
																		<xsd:enumeration value="2"/>
																		<xsd:enumeration value="3"/>
																		<xsd:enumeration value="4"/>
																		<xsd:enumeration value="5"/>
																		<xsd:enumeration value="6"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="PopulationSegmentsOperator" use="optional" default="1">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.PopulationSegmentsOperator
1 - OR
2 - AND</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:int">
																		<xsd:enumeration value="1"/>
																		<xsd:enumeration value="2"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="PopulationLocalSegmentsOperator" use="optional" default="1">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.PopulationLocalSegmentsOperator
1 - OR
2 - AND</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:int">
																		<xsd:enumeration value="1"/>
																		<xsd:enumeration value="2"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="PopulationOfflineMode" use="optional" default="1">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.PopulationOfflineMode
1 - Online only
2 - Also in offline
3 - Only in offline
4 - In offline disregard member segment condition</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:int">
																		<xsd:enumeration value="1"/>
																		<xsd:enumeration value="2"/>
																		<xsd:enumeration value="3"/>
																		<xsd:enumeration value="4"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="Reward" minOccurs="0">
														<xsd:complexType>
															<xsd:attribute name="TimingScope" use="optional" default="0">
																<xsd:annotation>
																	<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5010
0 - Immediate reward
1 - Defer reward to Member Account (supported only for RewardTemplateId=20)
2 - To payment voucher (currently not supported)</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:int">
																		<xsd:enumeration value="0"/>
																		<xsd:enumeration value="1"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="LimitUOMScope" use="optional" default="0">
																<xsd:annotation>
																	<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5032
When RewardTemplateId = 54:
0 - no limit on reward
1 - limit reward amount to
2 - maximum OUM on reward calculation</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:int">
																		<xsd:enumeration value="0"/>
																		<xsd:enumeration value="1"/>
																		<xsd:enumeration value="2"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="PercentLimitationScope" use="optional" default="0">
																<xsd:annotation>
																	<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5035
When RewardTemplateId in (10,61):
0 - No limit on reward
1 - Limit reward amount to
2 - Maximum spend on reward calculation</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:int">
																		<xsd:enumeration value="0"/>
																		<xsd:enumeration value="1"/>
																		<xsd:enumeration value="2"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="IncreaseMemberAccountType" use="optional" default="1">
																<xsd:annotation>
																	<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5031
1 - By units
2 - By percent
Mandatory for RewardTemplateId = 20</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:int">
																		<xsd:enumeration value="1"/>
																		<xsd:enumeration value="2"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="ManualRewardValueScope" use="optional" default="0">
																<xsd:annotation>
																	<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5120
0 - Fixed
1 - Maximum
Relevant and mandatory for PromotionTypeId in (11 ,12,13)</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:int">
																		<xsd:enumeration value="0"/>
																		<xsd:enumeration value="1"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="PromotionsBucket" minOccurs="0">
														<xsd:complexType>
															<xsd:attribute name="ThresholdAppliesTo" use="optional" default="0">
																<xsd:annotation>
																	<xsd:documentation>PromotionBucket.ThresholdAppliesTo
In case ThresholdTypeId != 5 and ThresholdTypeId != 9
0 - Any item
1 - Same items
2 - Different items

In case ThresholdTypeId = 5
0 - Current balance
101 - Opening balance
102 - Earning this visit
103 - Redemption This Visit

In case ThresholdTypeId = 9
201 - Any Tender</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:int">
																		<xsd:enumeration value="0"/>
																		<xsd:enumeration value="1"/>
																		<xsd:enumeration value="2"/>
																		<xsd:enumeration value="101"/>
																		<xsd:enumeration value="102"/>
																		<xsd:enumeration value="201"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="ParticipateInReward" type="xsd:boolean" use="optional" default="1">
																<xsd:annotation>
																	<xsd:documentation>PromotionBucket.ParticipateInReward
0 - not participating
1 - participating</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="AndOrOperator" type="xsd:boolean" use="optional" default="0">
																<xsd:annotation>
																	<xsd:documentation>PromotionBucket.AndOrOperator
0 - AND
1 - OR</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="AllocateDiscountBreakdown" type="xsd:boolean" use="optional" default="1">
																<xsd:annotation>
																	<xsd:documentation>PromotionBucket.AllocateDiscountBreakdown
0 - No allocation
1 - Allocation</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="BucketEntity" minOccurs="0">
														<xsd:complexType>
															<xsd:attribute name="AndOrOperator" type="xsd:boolean" use="optional" default="1">
																<xsd:annotation>
																	<xsd:documentation>PromotionBucketEntity.EntityAndOrOperator
0 - AND
1 - OR</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="Exclude" type="xsd:boolean" use="optional" default="0">
																<xsd:annotation>
																	<xsd:documentation>PromotionBucketEntity.Exclude
1 - Excluded
0 - Not excluded</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
														</xsd:complexType>
													</xsd:element>
												</xsd:all>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
									<xsd:attribute name="OfferProviderName">
										<xsd:annotation>
											<xsd:documentation>PromotionHeader.OfferProviderName
Allow distinguishing between different provider when sending reconciliation</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:minLength value="0"/>
												<xsd:maxLength value="100"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:attribute>
								</xsd:complexType>
							</xsd:element>
						</xsd:all>
						<xsd:attribute name="Id" type="xsd:string" use="required">
							<xsd:annotation>
								<xsd:documentation>From MP.RetailerCode WHERE TypeId = 0</xsd:documentation>
							</xsd:annotation>
						</xsd:attribute>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
		<xsd:key name="HQL_Promotion_InterfaceKey1">
			<xsd:selector xpath=".//Promotion"/>
			<xsd:field xpath="@PromotionHeaderId"/>
		</xsd:key>
	</xsd:element>
</xsd:schema>
