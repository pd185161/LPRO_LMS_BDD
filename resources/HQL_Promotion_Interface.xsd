																<xsd:simpleType>
																	<xsd:restriction base="xsd:short">
																		<xsd:enumeration value="0"/>
																		<xsd:enumeration value="1"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="MeanOfPaymentSharedTenderValue" type="xsd:float" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.MeanOfPaymentSharedTenderValue</xsd:documentation>
																</xsd:annotation>
