																	<xsd:documentation>PromotionHeader.MaximumItemPrice
FSD479817 - Support Minimum and Maximum Item Gross Price Limit</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:float">
																		<xsd:minInclusive value="0"/>
																		<xsd:maxInclusive value="999999999.999"/>
