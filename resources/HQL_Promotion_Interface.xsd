																											<xsd:enumeration value="5176"/>
																											<xsd:enumeration value="5178"/>
																											<xsd:enumeration value="5179"/>
																											<xsd:enumeration value="5180"/>
																											<xsd:enumeration value="5182"/>
																											<xsd:enumeration value="5183"/>
																											<xsd:enumeration value="5185"/>
																											<xsd:enumeration value="5045"/>
																											<xsd:enumeration value="5046"/>
																											<xsd:enumeration value="5047"/>
																											<xsd:enumeration value="5048"/>
																											<xsd:enumeration value="5049"/>
																											<xsd:enumeration value="5050"/>
																											<xsd:enumeration value="5051"/>
																											<xsd:enumeration value="5211"/>
																											<xsd:enumeration value="5212"/>
																											<xsd:enumeration value="5214"/>
																											<xsd:enumeration value="5215"/>
																											<xsd:enumeration value="5217"/>
																											<xsd:enumeration value="5218"/>
																											<xsd:enumeration value="5220"/>
																											<xsd:enumeration value="5221"/>
																											<xsd:enumeration value="5223"/>
																											<xsd:enumeration value="5142"/>
																											<xsd:enumeration value="5143"/>
																											<xsd:enumeration value="5144"/>
																											<xsd:enumeration value="5145"/>
																											<xsd:enumeration value="5146"/>
																											<xsd:enumeration value="5147"/>
																											<xsd:enumeration value="5148"/>
																										</xsd:restriction>
																									</xsd:simpleType>
