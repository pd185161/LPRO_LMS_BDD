																<xsd:annotation>
																	<xsd:documentation>
																		CR 522735
																		PromotionRegistration.DefaultRedemptionLimit
																		Relevant in case RegisteredPromotionRedemption is set to 2.
																		Default value for Registered Redemptions in Registered Promotion using Limit Set Per Household.
																		Can be defined from 1 – 999.
																	</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:int">
																		<xsd:minInclusive value="1"/>
																		<xsd:maxInclusive value="999"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:attribute>
															<xsd:attribute name="DisregardItemMinimumPrice" type="xsd:boolean" use="optional" default="0">
																<xsd:annotation>
																	<xsd:documentation>
																		EPIC LOYPRO-3827 Limit Reward to Item Minimum Price
																		PromotionHeader.DisregardItemMinimumPrice
																		Represents an indication whether Item Minimum Price should be ignored while providing reward.
																		1 - true, 0 - false
																		Not supported with ThresholdTypeId = 8 (Multi Case Quantity)
																	</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="Distribution" minOccurs="0">
														<xsd:annotation>
															<xsd:documentation>Populated into PromotionDistribution table</xsd:documentation>
														</xsd:annotation>
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="Store" maxOccurs="unbounded">
																	<xsd:complexType>
																		<xsd:attribute name="Id" type="xsd:string" use="required">
																			<xsd:annotation>
																				<xsd:documentation>PromotionDistribution.DistributedStoreInternalKey from HOST.StoreCode WHERE StoreCode.MatrixMemberId = ImportPromotions_DistributionToStores.MatrixMemberId AND StoreCode.StoreId = ImportPromotions_DistributionToStores.StoreId
Id=0 for all stores</xsd:documentation>
																			</xsd:annotation>
																		</xsd:attribute>
																		<xsd:attribute name="StartDate" type="restrictedDateTime" use="required">
																			<xsd:annotation>
																				<xsd:documentation>PromotionDistribution.StartDate</xsd:documentation>
																			</xsd:annotation>
																		</xsd:attribute>
																		<xsd:attribute name="EndDate" type="restrictedDateTime" use="required">
																			<xsd:annotation>
																				<xsd:documentation>PromotionDistribution.EndDate</xsd:documentation>
																			</xsd:annotation>
																		</xsd:attribute>
																		<xsd:attribute name="Suspended" type="xsd:boolean" use="optional">
																			<xsd:annotation>
																				<xsd:documentation>PromotionDistribution.Suspended</xsd:documentation>
																			</xsd:annotation>
																		</xsd:attribute>
																		<xsd:attribute name="Action" use="optional">
																			<xsd:annotation>
																				<xsd:documentation>Not saved in the promotion tables, it&apos;s an action on PromotionDistribution table
U = Update distribution
D = Delete store/Mark store as deleted from distribution</xsd:documentation>
																			</xsd:annotation>
																			<xsd:simpleType>
																				<xsd:restriction base="xsd:string">
																					<xsd:enumeration value="U"/>
																					<xsd:enumeration value="D"/>
																				</xsd:restriction>
																			</xsd:simpleType>
																		</xsd:attribute>
																	</xsd:complexType>
																</xsd:element>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="PromotionsLevels" minOccurs="0">
														<xsd:annotation>
															<xsd:documentation/>
														</xsd:annotation>
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element name="PromotionsLevel" maxOccurs="unbounded">
																	<xsd:complexType>
																		<xsd:all>
																			<xsd:element name="Reward">
																				<xsd:annotation>
																					<xsd:documentation>Populated into PromotionRewardAttributeValue table</xsd:documentation>
																				</xsd:annotation>
																				<xsd:complexType>
																					<xsd:attribute name="RewardTemplateId" use="required">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.RewardTemplateId

Triggered items:
1 - New price
2 - Amount off
10 - Percent discount
50 - Free item
51 - Price Comparison

TotalTicket:
60 - Amount off
61 - Percent off

UOM:
52 - New price
53 - Amount off
54 - Percent off
56 - Free item
57 - Price Comparison

Member Account:
20 - Earn Points/Credits/Visits
64 - Deduct Points/Credits/Visits

Coupon/Message:
55 - Coupon
62 - Target Message
63 - ExternalDeviceReward (currently not supported)
70 - Tender-Amount Off
71 - Tender-Percent Off</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:int">
																								<xsd:enumeration value="1"/>
																								<xsd:enumeration value="2"/>
																								<xsd:enumeration value="10"/>
																								<xsd:enumeration value="50"/>
																								<xsd:enumeration value="60"/>
																								<xsd:enumeration value="61"/>
																								<xsd:enumeration value="52"/>
																								<xsd:enumeration value="53"/>
																								<xsd:enumeration value="54"/>
																								<xsd:enumeration value="55"/>
																								<xsd:enumeration value="56"/>
																								<xsd:enumeration value="20"/>
																								<xsd:enumeration value="64"/>
																								<xsd:enumeration value="51"/>
																								<xsd:enumeration value="57"/>
																								<xsd:enumeration value="62"/>
																								<xsd:enumeration value="70"/>
																								<xsd:enumeration value="71"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="RewardValue">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5000 OR AttributeId = 5030 [Member Account Only]</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:string">
																								<xsd:maxLength value="50"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="RewardValueType" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5001
When RewardTemplateId = 1:
0 - Per item
2 - All items
3 - Per quantity of items

When RewardTemplateId = 2:
0 - for each rewarded item
2 - on total price  of grouped items

When RewardTemplateId = 52:
0 - on total sales of item
2 - on UOM price

When RewardTemplateId = 53:
0 - from UOM price
2 - from total sales of item</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:int">
																								<xsd:enumeration value="0"/>
																								<xsd:enumeration value="2"/>
																								<xsd:enumeration value="3"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="LimitDiscountScope" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5016
When RewardTemplateId in (1,2,10,52,53,54):
0 - All triggered items
1 - Limit item count</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:int">
																								<xsd:enumeration value="0"/>
																								<xsd:enumeration value="1"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="LimitItemScope" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5018
When RewardTemplateId in (1,2,10,50,52,53,54,56):
0 - Least expensive
1 - Most expensive</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:int">
																								<xsd:enumeration value="0"/>
																								<xsd:enumeration value="1"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="ConnectedSegmentId" use="optional">
																						<xsd:simpleType>
																							<xsd:annotation>
																								<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5152</xsd:documentation>
																							</xsd:annotation>
																							<xsd:restriction base="xsd:int">
																								<xsd:minInclusive value="1"/>
																								<xsd:maxInclusive value="999999999"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="DisconnectedSegmentId" use="optional">
																						<xsd:simpleType>
																							<xsd:annotation>
																								<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5154</xsd:documentation>
																							</xsd:annotation>
																							<xsd:restriction base="xsd:int">
																								<xsd:minInclusive value="1"/>
																								<xsd:maxInclusive value="999999999"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="IncreaseMemberAccountId" type="xsd:int" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5029
Mandatory for RewardTemplateId = 20</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="LimitItemCount" type="xsd:int" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5017
Relevant and mandatory in case RewardTemplateId in (1,2,10,50,52,53,54,56) and LimitDiscountScope = 1 - limit item count</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="TimingScope" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5010
0 - Immediate reward
1 - Defer  reward to Member Account(Supported only for RewardTemplateId=20)
2 - To payment voucher(currently not supported)</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:int">
																								<xsd:enumeration value="0"/>
																								<xsd:enumeration value="1"/>
																								<xsd:enumeration value="2"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="LimitUOMScope" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5032
When RewardTemplateId = 54:
0 - No limit on reward
1 - Limit reward amount to
2 - Maximum OUM on reward calculation</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:int">
																								<xsd:enumeration value="0"/>
																								<xsd:enumeration value="1"/>
																								<xsd:enumeration value="2"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="PercentLimitationScope" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5035
When RewardTemplateId in (10,61):
0 - No limit on reward
1 - Limit reward amount to
2 - Maximum spend on reward calculation</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:int">
																								<xsd:enumeration value="0"/>
																								<xsd:enumeration value="1"/>
																								<xsd:enumeration value="2"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="IncreaseMemberAccountType" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5031
1 - by units
2 - by percent
Mandatory for RewardTemplateId = 20</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:int">
																								<xsd:enumeration value="1"/>
																								<xsd:enumeration value="2"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="ManualRewardValueScope" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5120
0 - Fixed
1 - Maximum
Relevant and mandatory for PromotionTypeId in (11 ,12,13)</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:int">
																								<xsd:enumeration value="0"/>
																								<xsd:enumeration value="1"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="TriggerValueForIncentiveMsg" type="xsd:float" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5186
Trigger Value for incentive message (mandatory in case incentive message is defined in the Message element)</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="LimitRewardAmount" type="xsd:float" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5036
Relevant and mandatory when  RewardTemplateId in (10,61) and PercentLimitationScope = 1 (Limit reward amount to)</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="MaximumSpendForReward" type="xsd:float" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5037
Relevant and mandatory when  RewardTemplateId in (10,61) and PercentLimitationScope = 2 (Maximum spend on reward calculation)</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="ReduceMemberAccountScope" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5012
0 - Do not deduct
1 - Deduct from Member Account
2 - Zero Member Account
3 - Reset Init Value Member Account
4 - Deduct reward amount from MA
5 - Accumulate into Member Account</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:int">
																								<xsd:enumeration value="0"/>
																								<xsd:enumeration value="1"/>
																								<xsd:enumeration value="2"/>
																								<xsd:enumeration value="3"/>
																								<xsd:enumeration value="4"/>
																								<xsd:enumeration value="5"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="ReduceMemberAccountId" type="xsd:int" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5013
Relevant and mandatory in case ReduceMemberAccountScope in (1,2,3)</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="ReduceMemberAccountValue" type="xsd:float" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeID = 5014
Relevant and mandatory in case ReduceMemberAccountScope = 1 (Deduct from Member Account)</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="MSUQuantity" type="xsd:int" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5158
Relevant and mandatory when RewardValueType = 3</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="ReduceMemberAccountValidationAppliesTo" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5043
100 - Current balance
101 - Opening  balance
102 - Earnings this visit</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:int">
																								<xsd:enumeration value="100"/>
																								<xsd:enumeration value="101"/>
																								<xsd:enumeration value="102"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="ReducePriceScope" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5112
0 - Fix value reward
1 - Inject Reward from member account
3 - Inject Reward From item DA
4 - Inject Reward From Customer Account
5 - Inject Reward From Item Price</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:int">
																								<xsd:enumeration value="0"/>
																								<xsd:enumeration value="1"/>
																								<xsd:enumeration value="3"/>
																								<xsd:enumeration value="4"/>
																								<xsd:enumeration value="5"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="InjectRewardFromMAId" type="xsd:int" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5113</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="InjectRewardFromItemPrice" type="xsd:int" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5205</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="MABalanceType" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5114
100 - Current balance
101 - Opening  balance
102 - Earnings this visit</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:int">
																								<xsd:enumeration value="100"/>
																								<xsd:enumeration value="101"/>
																								<xsd:enumeration value="102"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="ReduceMAByRewardValue" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5118
0 - Not allowed
1 - Allowed</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:int">
																								<xsd:enumeration value="0"/>
																								<xsd:enumeration value="1"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="LimitRewardMAAmount" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5116
0 - Not allowed
1 - Allowed</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:int">
																								<xsd:enumeration value="0"/>
																								<xsd:enumeration value="1"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="LimitRewardMAScope" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5044
1 - to minimum new price of
2 - Amount to maximum of
3 - Percentage to maximum of</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:int">
																								<xsd:enumeration value="1"/>
																								<xsd:enumeration value="2"/>
																								<xsd:enumeration value="3"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="LimitRewardValue" type="xsd:float" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5117</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="CalculateRewardOnPurchasedAmount" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5134
0 - Rounded to the last trigger
1 - Exact amount
2 - Rounded to next trigger level</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:int">
																								<xsd:enumeration value="0"/>
																								<xsd:enumeration value="1"/>
																								<xsd:enumeration value="2"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="InjectRewardFromDAId" type="xsd:int" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5190</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="RewardEnteredByCashier" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5006
0 - not allowed By Cashier
1 - allowed By Cashier</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:int">
																								<xsd:enumeration value="0"/>
																								<xsd:enumeration value="1"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="AccumulateDiscountIntoMemberAccount" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5055
0 - No
1 - Yes</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:int">
																								<xsd:enumeration value="0"/>
																								<xsd:enumeration value="1"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="DiscountsAccumulatingMemberAccountId" type="xsd:int" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5052</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="CashierMaximumManualRewardSource" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5191
0 - Fixed
1 - Inject from member account. Relevant and mandatory when AttributeId 5120 = 1</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:int">
																								<xsd:enumeration value="0"/>
																								<xsd:enumeration value="1"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="CashierMaximumManualMAId" type="xsd:int" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5192
Relevant and mandatory when AttributeId  5191 = 1 - inject from member account</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="CashierMaximumManualMABalanceType" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5193
100 - Current balance
101 - Opening balance
102 - Earnings this visit
Relevant and mandatory when AttributeId  5191 = 1 - inject from member account</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:int">
																								<xsd:enumeration value="100"/>
																								<xsd:enumeration value="101"/>
																								<xsd:enumeration value="102"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="ReduceMemberAccountValueScope" type="xsd:int" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5194
1 - Value
2 - Amount</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="CalculatePromotionRewardExcludeTax" type="xsd:int" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5195
0 - Calculate Promotion Reward with Tax
1 - Calculate Promotion Reward without Tax</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="AddTaxAmountToRewardAmount" type="xsd:int" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5196
0 - Do not Add Tax to Reward Amount
1 - Add Tax to Reward Amount</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="TimingMemberAccountScope" type="xsd:int" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5011
Contains Account ID.
Relevant and mandatory in case TimingScope=1 (Defer Reward to Member Account)</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="RewardPointsExpirationValue" type="xsd:int" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5354
This attribute is supported in the center but not in LPE
Contains Account Expiration value (number of days or month untill expiration for the value added to the account), reward member account section</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="AdditionalPointsExpirationValue" type="xsd:int" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5355
This attribute is supported in the center but not in LPE
Contains Account Expiration value (number of days or month untill expiration for the value added to the account), reward update member account balance section</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="RewardIntoMAExpirationValue" type="xsd:int" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5356
This attribute is supported in the center but not in LPE
Contains Account Expiration value (number of days or month untill expiration for the value added to the account), reward timing section</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="IssueDigitalCoupon" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5197</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:token">
																								<xsd:maxLength value="50"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="DigitalCouponRequired" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5198
0 - Not required
1 - Required</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:byte">
																								<xsd:enumeration value="0"/>
																								<xsd:enumeration value="1"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="ManualEntryAllowed" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5206
0 - Not allowed
1 - Allowed</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:byte">
																								<xsd:enumeration value="0"/>
																								<xsd:enumeration value="1"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="DocumentType" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE Attribute ID = 5129
5 - Coupon
10 - Voucher
Should not be sent when using Alpha Nomeric coupon</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:byte">
																								<xsd:enumeration value="5"/>
																								<xsd:enumeration value="10"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="DocumentId" type="xsd:int" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE Attribute Id = 5025 (TimingVoucherId)</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="DocumentValidScope" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE Attribute ID = 5122 (VoucherValidScope)
1 - Valid for X days
2 - Valid between dates
3 - Always</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:byte">
																								<xsd:enumeration value="1"/>
																								<xsd:enumeration value="2"/>
																								<xsd:enumeration value="3"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="DocumentStartingFrom" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE Attribute ID = 5123 (VoucherStartingFrom)
0 - Issue Date
1 - 1 Day from Issuing
2 - 1 Week From issuing
3 - 2 weeks from issuing
4 - 1st on next month</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:byte">
																								<xsd:enumeration value="0"/>
																								<xsd:enumeration value="1"/>
																								<xsd:enumeration value="2"/>
																								<xsd:enumeration value="3"/>
																								<xsd:enumeration value="4"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="DocumentValidForXDays" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE Attribute Id = 5124 (VoucherValidForXDays)</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:int">
																								<xsd:minInclusive value="1"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="DocumentValidFrom" type="xsd:date" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE Attribute Id = 5125 (VoucherStartingFrom)</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="DocumentValidTo" type="xsd:date" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE Attribute Id = 5126 (VoucherValidTo)</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="IssueOmniChannelCoupon" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5207
Indicates whether an omni channel coupon is issued in this promotion</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:byte">
																								<xsd:enumeration value="0"/>
																								<xsd:enumeration value="1"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="MinimumManualMAValue" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5224</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:float">
																								<xsd:minInclusive value="0.001"/>
																								<xsd:maxInclusive value="999999999.999"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="MinimumManualMABalanceType" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5225
100 - Current balance
101 - Opening balance
102 - Earnings this visit</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:short">
																								<xsd:enumeration value="100"/>
																								<xsd:enumeration value="101"/>
																								<xsd:enumeration value="102"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="TaxExecutionMethod" type="xsd:int" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5226
1 - As Discount
2 - As Mean of Payment</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="TenderIDForTax" type="xsd:int" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5227</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="CampaignLimitsScope" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5445
0 - No Campaign Limits
1 - Maximum Promotion Redemptions
2 - Maximum Promotion Rewards</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:int">
																								<xsd:enumeration value="0"/>
																								<xsd:enumeration value="1"/>
																								<xsd:enumeration value="2"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="MaximumPromotionRedemptions" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5442</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:int">
																								<xsd:minInclusive value="1"/>
																								<xsd:maxInclusive value="999999999"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="MaximumPromotionRewards" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5443</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:double">
																								<xsd:minInclusive value="0"/>
																								<xsd:maxInclusive value="99999999"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="IssueCompensationDocumentScope" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5228
0 - Never
1 - Offline
2 - Not enough barcodes
3 - Offline or not enough barcodes</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:byte">
																								<xsd:enumeration value="0"/>
																								<xsd:enumeration value="1"/>
																								<xsd:enumeration value="2"/>
																								<xsd:enumeration value="3"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="CompensationDocumentId" type="xsd:int" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5229
The document id of the compensation coupon/voucher</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="CompensationValidScope" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5230
1 - Valid for X days
2 - Valid between dates
3 - Always</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:byte">
																								<xsd:enumeration value="1"/>
																								<xsd:enumeration value="2"/>
																								<xsd:enumeration value="3"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="CompensationStartingFrom" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5231
0 - Issue date
1 - One day from issuing
2 - One week from issuing
3 - 2 weeks from issuing
4 - 1 on next month
Relevant if attribute CompensationValidScope = 1</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:byte">
																								<xsd:enumeration value="0"/>
																								<xsd:enumeration value="1"/>
																								<xsd:enumeration value="2"/>
																								<xsd:enumeration value="3"/>
																								<xsd:enumeration value="4"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="CompensationValidForXDays" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5232
Relevant if attribute CompensationValidScope = 1</xsd:documentation>
																						</xsd:annotation>
																						<xsd:simpleType>
																							<xsd:restriction base="xsd:int">
																								<xsd:minInclusive value="1"/>
																							</xsd:restriction>
																						</xsd:simpleType>
																					</xsd:attribute>
																					<xsd:attribute name="CompensationValidFrom" type="xsd:date" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5233
Relevant if attribute CompensationValidScope = 2</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="CompensationValidTo" type="xsd:date" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5234
Relevant if attribute CompensationValidScope = 2</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="RecalculateTaxAfterPromotionDiscount">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5199
CRL 505739 Add Recalculate Tax after promotions discount attribute to promotion builder</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="RewardLimitationScope" type="xsd:int" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeId = 5242
Used When RewardTemplateId in (2, 60, 53, 70, 20)
0 - No limit on reward
1 - Limit reward amount to
3 – Limit reward amount down to 0.01 balance</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																					<xsd:attribute name="RewardLimitationAmount" type="xsd:float" use="optional">
																						<xsd:annotation>
																							<xsd:documentation>PromotionRewardAttributeValue.AttributeValue WHERE AttributeID = 5243. 
Used When RewardTemplateId in (2, 60, 53, 70, 20) 
AND PromotionRewardAttributeValue.AttributeValue (AttributeID = 5242) = ‘1’</xsd:documentation>
																						</xsd:annotation>
																					</xsd:attribute>
																				</xsd:complexType>
																			</xsd:element>
																			<xsd:element name="Thresholds">
																				<xsd:annotation>
																					<xsd:documentation>Populated into PromotionBucketThreshold table</xsd:documentation>
																				</xsd:annotation>
																				<xsd:complexType>
																					<xsd:sequence>
