															<xsd:attribute name="CalculatePromotionPostTax" type="xsd:boolean" use="optional">
																<xsd:annotation>
																	<xsd:documentation>PromotionHeader.CalculatePromotionPostTax</xsd:documentation>
																</xsd:annotation>
															</xsd:attribute>
															<xsd:attribute name="MinimumItemPrice" use="optional">
